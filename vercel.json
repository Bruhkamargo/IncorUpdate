{
    "version": 2,
    "builds": [
      { "src": "vite.config.js", "use": "@vercel/static-build" },
      { "src": "jsPDF/generated-files/**", "use": "@vercel/static" }
    ],
    "routes": [
        {
            "src": "/[^.]+",
            "dest": "/",
            "status": 200
        }
    ]
}